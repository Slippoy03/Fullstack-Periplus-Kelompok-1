{
  "version": 2,
  "builds": [
    { "src": "backend/api/**/*.js", "use": "@vercel/node" },
    {
      "src": "frontend/Periplus-Fullstack-Project/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "backend/api/**/*.js", "dest": "backend/api/$1" },
    { "src": "/(.*)", "dest": "/frontend/Periplus-Fullstack-Project/$1" }
  ]
}
